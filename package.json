{
  "name": "express-vue-template",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Mostafa Rowghanian <mostafatech@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-client": "cross-env NODE_ENV=production webpack --config=./client/webpack.config.js",
    "watch-client": "cross-env NODE_ENV=development webpack --watch --config=./client/webpack.config.js",
    "watch-server": "nodemon ./server/main.js",
    "dev": "npm run watch-client | npm run watch-server",
    "start": "npm run build-client | node ./server/main.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "vue": "^2.5.11",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "edge.js": "^1.1.4",
    "express": "^4.16.4",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.23.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0"
  }
}
